{
  "name": "sketch-replace-images-text",
  "version": "0.5.2",
  "license": "Apache-2.0",
  "description": "Sketch 45+ plugin to replace (as in update) existing images by name in a sketch document.",
  "identifier": "us.appby.sketch-replace-images-text",
  "author": "hellobanny",
  "authorEmail": "info@hellobanny.de",
  "bugs": "https://github.com/hellobanny/sketch-replace-images/issues",
  "contributors": "hellobanny",
  "keywords": [
    "sketch, plugin, replace images, replace"
  ],
  "engines": {
    "sketch": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hellobanny/sketch-replace-images.git"
  },
  "skpm": {
    "name": "Sketch Replace Images & Text",
    "manifest": "src/manifest.json",
    "main": "sketch-replace-images.sketchplugin"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.10",
    "npm": "^6.4.1",
    "skpm": "*"
  }
}
